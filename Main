package Typics;

import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner userInput = new Scanner(System.in);
		System.out.println("Welcome to the Typics System");
		System.out.println("Please enter as appropriate: Admin, Alumni, Lecturer, Student, Staff, Authors");
		String UserResponse = userInput.next(); 
		UserResponse = UserResponse.toUpperCase();
		
		switch(UserResponse) {
		case "ADMIN":
		
			System.out.println("Check entry!!!");
		}
		}
		
	}

}}
